{
  "name": "soen-390",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "prettier": "prettier --write **/*.{js,ts,tsx,json,md,yml,yaml}",
    "cypress": "cross-env NODE_ENV=test cypress open",
    "cypress:headless": "cross-env NODE_ENV=test cypress run",
    "test:cypress:component": "cross-env NODE_ENV=test cypress run --component",
    "test:cypress:e2e": "cross-env NODE_ENV=test start-server-and-test dev http://localhost:3000 cypress:headless",
    "test:jest": "cross-env NODE_ENV=test jest ./tests --verbose",
    "test": "npm run test:jest && npm run test:cypress:component && npm run test:cypress:e2e",
    "reports:copy": "mkdir coverage .nyc_output || true && cp cy-coverage/coverage-final.json coverage/from-cypress.json && cp jest-coverage/coverage-final.json coverage/from-jest.json",
    "reports:combine": "npx nyc merge coverage && mv coverage.json .nyc_output/out.json",
    "reports:report": "npx nyc report --reporter html --reporter text --reporter lcovonly --report-dir coverage",
    "coverage": "npm run reports:copy && npm run reports:combine && npm run reports:report"
  },
  "nyc": {
    "report-dir": "cy-coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
