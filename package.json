{
  "name": "soen-390",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "prettier": "prettier --write .",
    "cypress": "NODE_ENV=test cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "test:cypress:component": "NODE_ENV=test cypress run --component",
    "test:cypress:e2e": "NODE_ENV=test start-server-and-test dev http://localhost:3000 cypress:headless",
    "test:jest": "NODE_ENV=test jest ./tests --verbose",
    "test": "npm run test:jest && npm run test:cypress:component && npm run test:cypress:e2e",
    "reports:copy": "mkdir coverage .nyc_output || true && cp cy-coverage/coverage-final.json coverage/from-cypress.json && cp jest-coverage/coverage-final.json coverage/from-jest.json",
    "reports:combine": "npx nyc merge coverage && mv coverage.json .nyc_output/out.json",
    "reports:report": "npx nyc report --reporter html --reporter text --reporter cobertura --report-dir coverage",
    "coverage": "npm run reports:copy && npm run reports:combine && npm run reports:report"
  },
  "nyc": {
    "report-dir": "cy-coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.8.0",
    "@tanstack/react-query": "^4.20.0",
    "@trpc/client": "^10.8.1",
    "@trpc/next": "^10.8.1",
    "@trpc/react-query": "^10.8.1",
    "@trpc/server": "^10.8.1",
    "framer-motion": "^8.5.4",
    "next": "13.1.1",
    "next-auth": "^4.18.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "start-server-and-test": "^1.15.3",
    "superjson": "1.9.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@bahmutov/cypress-code-coverage": "^1.3.2",
    "@cypress/code-coverage": "^3.10.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "autoprefixer": "^10.4.7",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.4.1",
    "eslint": "^8.30.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "postcss": "^8.4.14",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.8.0",
    "tailwindcss": "^3.2.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.3.1"
  }
